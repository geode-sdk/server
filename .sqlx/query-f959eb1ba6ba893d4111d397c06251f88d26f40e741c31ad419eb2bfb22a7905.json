  "db_name": "PostgreSQL",
  "query": "UPDATE mod_versions mv\n            SET name = $1,\n            version = $2,\n            download_link = $3,\n            hash = $4,\n            geode = $5,\n            early_load = $6,\n            api = $7,\n            description = $8,\n            updated_at = NOW()\n        FROM mod_version_statuses mvs\n        WHERE mv.status_id = mvs.id\n        AND mvs.status = 'pending'\n        AND mv.id = $9\n        RETURNING mv.id,\n            name,\n            version,\n            download_link,\n            download_count,\n            hash,\n            geode,\n            early_load,\n            api,\n            status_id,\n            description,\n            mod_id,\n            mv.created_at,\n            mv.updated_at",
